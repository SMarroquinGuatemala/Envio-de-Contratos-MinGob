                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        B.DPI AS dpiFirmantePatrono, C.Nombre AS razonSocialPatrono, B.Nombres + '' + B.Apellidos AS nombreFirmantePatrono, 'Representante' AS calidadFirmanteLegalPatrono, A.Salario, 5 AS jornada, 
                         CASE WHEN A.Empresa = 1 THEN 1 ELSE 2 END AS actividadEconomica, CASE WHEN E.Catorcenal = 1 THEN 2 ELSE 3 END AS formaPago, CASE WHEN A.TipoDeContrato = '02' THEN 1 ELSE 2 END AS plazo, 
                         A.FechaDeIngreso AS fechaInicioRelacion, A.FechaDeEgreso AS fechaFin, 0 AS horasTiempoParcial, 0 AS tarifaTiempoParcial, '' AS jornadaTiempoParcial, A.Nombres + ' ' + A.Apellidos AS nombreEmpleado, 
                         A.DPI AS dpiEmpleado, dbo.FnCalcularEdad(A.FechaDeNacimiento) AS edadEmpleado, A.Sexo AS sexoEmpleado, A.NumeroDeEmpleado, A.Planilla, A.Puesto, A.Empresa
FROM            TblPersonal AS A INNER JOIN
                         TblResponsableRRHH AS B ON B.Empresa = A.Empresa INNER JOIN
                         VwEmpresas AS C ON C.Empresa = A.Empresa INNER JOIN
                         TblPlanillas AS D ON D.Planilla = A.Planilla AND D.Empresa = A.Empresa INNER JOIN
                         TblTiposDePlanilla AS E ON E.TipoDePlanilla = D.TipoDePlanilla AND E.Empresa = D.Empresa
WHERE        (A.NumeroDeEmpleado = ISNULL(@PNumeroDeEmpleado, A.NumeroDeEmpleado)) AND (A.FechaDeEgreso IS NULL)</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="PNumeroDeEmpleado" ColumnName="NumeroDeEmpleado" DataSourceName="DbRecursosHumanos.dbo.TblPersonal" DataTypeServer="char(7)" DbType="AnsiString" Direction="Input" ParameterName="@PNumeroDeEmpleado" Precision="0" ProviderType="Char" Scale="0" Size="7" SourceColumn="NumeroDeEmpleado" SourceColumnNullMapping="false" SourceVersion="Current" />
